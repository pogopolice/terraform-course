terraform {
  backend "remote" {
    organization = "example-org-211007"
    workspaces {
      name = "Example-Workspace"
    }
  }
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 3.27"
    }
  }

  required_version = ">= 0.14.9"
}

provider "aws" {
  profile = "javaapp"
  region  = var.AWS_REGION
}
